#include <stdio.h>
int main()
{
   int maths, phy, chem, eng, hindi;
   float percentage=0;
   printf(" Mathematics : ");
   scanf("%d", &maths);
   printf(" physics : ");
   scanf("%d", &phy);
   printf(" Chemistry : ");
   scanf("%d", &chem);
   printf(" English : ");
   scanf("%d", &eng);
   printf(" Hindi : ");
   scanf("%d", &hindi);
   percentage =((maths+phy+chem+eng+hindi)*100)/500;
   printf("\nThe percentage of the student is : %f %", percentage);
   return 0;
}
